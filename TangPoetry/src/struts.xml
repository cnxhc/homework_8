<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.enable.DynamicMethodInvocation" value="true"></constant>
	<package name="tang" extends="struts-default">
		<action name="*" class="queryAction" method="{1}">
			<result name="byNameSuccess">queryByNameSuccess.jsp</result>
			<result name="byNameFail">queryByNameFail.jsp</result>
			<result name="byTitleSuccess">queryByTitleSuccess.jsp</result>
			<result name="byTitleFail">queryByTitleFail.jsp</result>
			<result name="byContentSuccess">queryByContentSuccess.jsp</result>
			<result name="byContentFail">queryByContentFail.jsp</result>
		</action>
	</package>
</struts>    
